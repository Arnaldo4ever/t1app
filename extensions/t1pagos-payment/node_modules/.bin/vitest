#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/vitest@0.29.8/node_modules/vitest/node_modules:/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/vitest@0.29.8/node_modules:/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/vitest@0.29.8/node_modules/vitest/node_modules:/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/vitest@0.29.8/node_modules:/home/arnaldo/Documentos/Extra/T1pagos/t1pagos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
